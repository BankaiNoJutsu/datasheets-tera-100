			<ExpData level="0" exp="1"/>
			<ExpData level="1" exp="50"/>
			<ExpData level="10" exp="50"/>
		</LevelUpGainExpData>
		<EpItemUseExpData>
			<!-- 테스트 용도로만 사용. QA 후 삭제 예정  //-->
			<ExpData itemTemplateId="96010" exp="500" />
			<!-- //-->
		</EpItemUseExpData>
