<?xml version="1.0" encoding="utf-8"?>
<DungeonRookieEvent>
	<RookieCondition clearCount="5" />
	<Parcel sender="@parcel:3" title="@parcel:112" msg="@parcel:1112" />
	
	<!--
		테라박스에서 사용할 임시 데이터.
		QA Command로만 데이터를 읽어 사용 가능(LIVE에서 사용하지 않는 데이터)
		시간은 UTC시간으로 입력할 것(한국은 UTC+9 이므로 실제시간 -9 해주면 됨)
	-->
	<SampleEventList>
		<Event dungeonId="9916" startTime="20161202 0000" endTime="20161207 0000" >
			<RewardItem templateId="1" amount="1"/>
			<RewardItem templateId="2" amount="1"/>
			<RewardItem templateId="3" amount="10"/>
		</Event>
		<Event dungeonId="9770" startTime="20161202 0000" endTime="20161207 0000" >
			<RewardItem templateId="1" amount="1"/>
			<RewardItem templateId="2" amount="1"/>
			<RewardItem templateId="3" amount="10"/>
		</Event>
		<Event dungeonId="9777" startTime="20161202 0000" endTime="20161209 0000" >
			<RewardItem templateId="1" amount="1"/>
			<RewardItem templateId="2" amount="1"/>
			<RewardItem templateId="3" amount="10"/>
		</Event>
	</SampleEventList>
	
	<!-- 매칭 가이드 -->
	<EventDialog>
		<Event name="MatchOn" DialogId="800100001"/>
		<Event name="MatchOff" DialogId="800100002"/>
		<Event name="ReMatch" DialogId="800100001"/>
	</EventDialog>
</DungeonRookieEvent>
