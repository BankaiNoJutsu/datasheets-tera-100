			<BattleChipTime day="Thursday" openHour="22" openHourQa="10" closeHour="24"  />
			<BattleChipTime day="Thursday" openHour="00" openHourQa="10" closeHour="02"  />

			
			<BattleChipTime day="Friday" openHour="22" openHourQa="10" closeHour="24"  />
			<BattleChipTime day="Friday" openHour="00" openHourQa="10" closeHour="02"  />

			
			<BattleChipTime day="Saturday" openHour="18" openHourQa="10" closeHour="24"  />
			<BattleChipTime day="Saturday" openHour="00" openHourQa="10" closeHour="02"  />

			
